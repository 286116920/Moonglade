@model SubscriptionViewModel

@foreach (var (key, value) in Model.Cats.OrderBy(c => c.Key))
{
    <div class="col-6">
        <div class="tilexsm">
            <div class="tilexsm-image">
                <i class="fas fa-rss"></i>
            </div>

            <div class="tilexsm-info">
                <div class="tilesm-title">
                    <a asp-controller="Subscription" asp-action="Rss" asp-route-categoryName="@value"
                       title="@key">
                        @key
                    </a>
                </div>
            </div>
        </div>
    </div>
}
@model IReadOnlyList<Moonglade.Data.Entities.FriendLinkEntity>

@if (Model.Any())
{
    <ul class="list-unstyled">
        @foreach (var item in Model)
        {
            <li>
                <a href="@item.LinkUrl" target="_blank">
                    @item.Title
                </a>
            </li>
        }
    </ul>
}